public class Solution {
    public List<String> fizzBuzz(int n) {
        List<String> list = new  ArrayList();
		String f = "Fizz",b = "Buzz";
		for(int i=1;i<=n;i++){
			if(i%3==0 && i%5==0)
				list.add(f+b);				
			else if(i%3==0) list.add(f);
			else if(i%5==0) list.add(b);
			else list.add(i+"");
		}
        return list;
    }
}
